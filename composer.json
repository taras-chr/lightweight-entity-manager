{
    "name": "taras-chr/lightweight-entity-manager",
    "description": "Lightweight entity manager for mapping your data into classes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taras Chornyi",
            "email": "taras.chr@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "EntityManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EntityManager\\Example\\": "example/"
        }
    },
    "require": {
        "php": "^7.1",
        "doctrine/collections": "^1.6",
        "doctrine/annotations": "^1.8"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0@dev",
        "phpunit/phpunit": "^8",
        "phpunit/php-invoker": "^2.0@dev"
    }
}
